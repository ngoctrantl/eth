{
    "maven.view": "hierarchical",
    "csharp.suppressDotnetInstallWarning": true,
    "boot-java.change-detection.on": true,
    "boot-java.support-spring-xml-config.on": true,
    "browser-preview.verbose": true,
    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,
    "eslint.provideLintTask": true,
    "eslint.quiet": true,
    "go.autocompleteUnimportedPackages": true,
    "go.coverOnSave": true,
    "go.coverOnSingleTest": true,
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "html.format.indentHandlebars": true,
    "gitProjectManager.displayProjectPath": true,
    "installVSIX.force": true,
    "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
    "java.codeGeneration.generateComments": true,
    "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
    "jshint.lintHTML": true,
    "maven.terminal.useJavaHome": true,
    "mdk.createUploadBundleZip": true,
    "nativescript.analytics.enabled": true,
    "path-intellisense.autoSlashAfterDirectory": true,
    "path-intellisense.showHiddenFiles": true,
    "npm.enableScriptExplorer": true,
    "npm.runSilent": true,
    "npm-intellisense.packageSubfoldersIntellisense": true,
    "ontology.compile.pythonCompiler2": true,
    "path-intellisense.extensionOnImport": true,
    "path-autocomplete.extensionOnImport": true,
    "rust-client.logToFile": true,
    "search-node-modules.useLastFolder": true,
    "secbit.asERC20": true,
    "secbit.noSMT": true,
    "secbit.onSave": true,
    "solidity-va.diagnostics.cdili_json.import": true,
    "sqflint.checkPaths": true,
    "tslint.alwaysShowStatus": true,
    "javascript.format.insertSpaceAfterConstructor": true,
    "pddlPlanner.executableOrService": "http://solver.planning.domains/solve"
}